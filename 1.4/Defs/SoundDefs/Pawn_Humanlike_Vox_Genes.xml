<Defs>

  <!-- Slug -->
  <SoundDef>
    <defName>AG_Pawn_Slug_Call</defName>
    <context>MapOnly</context>
    <subSounds>
      <li>
        <grains>
          <li Class="AudioGrain_Folder">
            <clipFolderPath>AG_SlugVoice/Call</clipFolderPath>
          </li>
        </grains>
        <volumeRange>20</volumeRange>
        <pitchRange>0.95~1.05</pitchRange>
        <sustainLoop>False</sustainLoop>
      </li>
    </subSounds>
  </SoundDef>

