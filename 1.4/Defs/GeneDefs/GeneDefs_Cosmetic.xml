	<GeneDef ParentName="GeneHeadboneBase">
		<defName>AG_BullHorns</defName>
		<label>bull horns</label>
		<displayCategory>AG_Cosmetic_Horns</displayCategory>
		<description>Carriers of this gene grow two large bull horns protruding from the forehead. These are not purely cosmetic, as they can be used for offensive purposes.</description>
		<iconPath>UI/Icons/Genes/AG_BullHornsIcon</iconPath>
		<selectionWeight>0</selectionWeight>
		<geneClass>VanillaGenesExpanded.Gene_AddsHediff</geneClass>
		<customEffectDescriptions>
			<li>Increases the carrier's dps by adding a 16 Stab damage, 2 seconds cooldown melee attack</li>
			
		</customEffectDescriptions>
		<graphicData>
			<graphicPath>Things/Pawn/Humanlike/HeadAttachments/AG_BullHorns/AG_BullHorns</graphicPath>
			<color>(255, 255, 255)</color>
			<layer>PostHeadgear</layer>
		</graphicData>
		<symbolPack>
			<prefixSymbols>
				<li>
					<symbol>bull</symbol>
				</li>
				<li>
					<symbol>uro</symbol>
				</li>
				<li>
					<symbol>oxen</symbol>
				</li>
				<li>
					<symbol>longhorn</symbol>
				</li>
			</prefixSymbols>
			<suffixSymbols>
				<li>
					<symbol>horn</symbol>
				</li>
			</suffixSymbols>
		</symbolPack>
		<modExtensions>
			<li Class="VanillaGenesExpanded.GeneExtension">
				<backgroundPathEndogenes>UI/Icons/Genes/AG_Endogenes</backgroundPathEndogenes>
				<backgroundPathXenogenes>UI/Icons/Genes/AG_Xenogenes</backgroundPathXenogenes>
				<hediffsToBodyParts>
					<li>
						<hediff>AG_HornAttack</hediff>
						<bodyparts><li>Head</li></bodyparts>					
					</li>			
				</hediffsToBodyParts>
			</li>
		</modExtensions>
	</GeneDef>
