	<GeneDef ParentName="GeneImmunityBase">
		<defName>AG_Gene_VeryWeakImmunity</defName>
		<label>very weak immunity</label>
		<labelShortAdj>sickly</labelShortAdj>
		<description>Carriers of this gene gain immunity to diseases very slowly. They will die from infections that others would survive.</description>
		<iconPath>UI/Icons/Genes/AG_Gene_VeryWeakImmunity</iconPath>
		<displayOrderInCategory>0</displayOrderInCategory>
		<statFactors>
			<ImmunityGainSpeed>0.75</ImmunityGainSpeed>
		</statFactors>
		<biostatMet>4</biostatMet>
		<selectionWeight>0</selectionWeight>
		<symbolPack>
			<prefixSymbols>
				<li>
					<symbol>sick</symbol>
					<weight>3</weight>
				</li>
				<li>
					<symbol>frail</symbol>
				</li>
			</prefixSymbols>
		</symbolPack>
		<modExtensions>
			<li Class="VanillaGenesExpanded.GeneExtension">
				<backgroundPathEndogenes>UI/Icons/Genes/AG_Endogenes</backgroundPathEndogenes>
				<backgroundPathXenogenes>UI/Icons/Genes/AG_Xenogenes</backgroundPathXenogenes>
			</li>
		</modExtensions>
	</GeneDef>
</Defs>