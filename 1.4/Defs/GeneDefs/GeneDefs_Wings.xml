	<GeneDef ParentName="AG_WingBase">
		<defName>AG_RainbowWings</defName>
		<label>rainbow wings</label>
		<description>Carriers of this gene have a pair of lepidopteran giant wings sprouting from their back.</description>
		<iconPath>UI/Icons/Genes/AG_RainbowWingsIcon</iconPath>
		<biostatCpx>1</biostatCpx>
		<displayCategory>AG_Wings</displayCategory>
		<customEffectDescriptions>
			<li>Colonist will ignore terrain movement costs</li>
			
		</customEffectDescriptions>
		<biostatMet>-1</biostatMet>
		<selectionWeight>0</selectionWeight>
		<geneClass>VanillaGenesExpanded.Gene_AddsHediff</geneClass>
		<symbolPack>
			<prefixSymbols>
				<li>
					<symbol>wing</symbol>
				</li>
				<li>
					<symbol>butterf</symbol>
				</li>
				<li>
					<symbol>colour</symbol>
				</li>
				<li>
					<symbol>rainb</symbol>
				</li>
				
			</prefixSymbols>
		</symbolPack>
		<graphicData>
			<graphicPath>Things/Pawn/Humanlike/BodyAttachments/AG_RainbowWings/AG_RainbowWings</graphicPath>
			<drawScale>2</drawScale>
			<drawOffsetNorth>(0, 0.19, 0)</drawOffsetNorth>
			<drawOffsetSouth>(0, 0, 0)</drawOffsetSouth>
			<drawOffsetEast>(0, 0, 0)</drawOffsetEast>
		</graphicData>
		<abilities>
			<li>AG_WingPoweredJump</li>
		</abilities>
		<modExtensions>
			<li Class="VanillaGenesExpanded.GeneExtension">
				<backgroundPathEndogenes>UI/Icons/Genes/AG_Endogenes</backgroundPathEndogenes>
				<backgroundPathXenogenes>UI/Icons/Genes/AG_Xenogenes</backgroundPathXenogenes>
				<hediffToWholeBody>AG_Hover</hediffToWholeBody>
			</li>
		</modExtensions>
	</GeneDef>
