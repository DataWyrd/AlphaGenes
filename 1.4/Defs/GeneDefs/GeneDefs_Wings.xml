	<GeneDef Name="AG_WingBase" Abstract="True">
		<displayCategory>Miscellaneous</displayCategory>
		<exclusionTags>
			<li>Wing</li>
			<li>Wings</li>
		</exclusionTags>
		<graphicData>
			<drawLoc>Tailbone</drawLoc>
