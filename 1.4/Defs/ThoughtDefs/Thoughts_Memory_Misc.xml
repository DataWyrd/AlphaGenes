<?xml version="1.0" encoding="utf-8" ?>
<Defs>



	
	
	<ThoughtDef>
		<defName>AG_Disgust</defName>
		<durationDays>1</durationDays>
		<stackLimit>1</stackLimit>
		<stages>
			<li>
				<label>disgusting colonist</label>
				<description>I can't get it out of my mind. How can anyone be SO revolting and grotesque?</description>
				<baseMoodEffect>-5</baseMoodEffect>
			</li>
		</stages>
		<nullifyingGenes><li>AG_Beauty_VeryVeryUgly</li></nullifyingGenes>
	</ThoughtDef>
	
	<ThoughtDef>
		<defName>AG_Awe</defName>
		<durationDays>1</durationDays>
		<stackLimit>1</stackLimit>
		<stages>
			<li>
				<label>angelic colonist</label>
				<description>I can't get it out of my mind. How can anyone be SO beautiful?</description>
				<baseMoodEffect>5</baseMoodEffect>
			</li>
		</stages>
		
	</ThoughtDef>
	
	<ThoughtDef>
		<defName>AG_CorpseFeeding</defName>
		<durationDays>1</durationDays>
		<stackLimit>1</stackLimit>
		<stages>
			<li>
				<label>smell of corpse-rotting gas</label>
				<description>The smell of putrid corpse bile and corpse-rotting gas is overpowering!</description>
				<baseMoodEffect>-10</baseMoodEffect>
			</li>
		</stages>
		<nullifyingGenes><li>AG_CorpseFeeding</li></nullifyingGenes>
	</ThoughtDef>



</Defs>
